<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="proberConfig" type="DNSProbe.ProberConfig, DNSProbe, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" allowLocation="true" allowDefinition="Everywhere" allowExeDefinition="MachineToApplication" restartOnExternalChanges="true"/>
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler"/>
  </configSections>
  
  <connectionStrings>
<add name="dns_result_form" connectionString="server=219.245.186.180;database=imiss_dns;user id=sa;password=imiss@108;" />
  </connectionStrings>

<proberConfig proberIP="192.168.2.250" defaultGw="192.168.2.1" fakePortBase="4000" localDNS="202.117.0.20" processNumForDig="10" digIteration="2" maxSendRate="10000" maxTaskNum="10" sendThreadNumByThousand="1" EnableLog="true" extractThreadNum="1"/>


  <log4net>
    <!--Define the output to a file-->
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <lockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <file value="log\"/>
      <appendToFile value="true"/>
      <rollingStyle value="Date"/>
      <datePattern value="yyyyMMdd&quot;.txt&quot;"/>
      <StaticLogFileName value="false"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="Time:%date Level:%p  content:%message%newline"/>
      </layout>
    </appender>
    <!--Define the output to the console command line-->
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] - %message%newline"/>
      </layout>
    </appender>
    <!--Define the log output media-->
    <root>
      <level value="ALL"/>
      <!--Log file format-->
      <appender-ref ref="LogFileAppender"/>
      <!--Display log-->
      <appender-ref ref="ConsoleAppender"/>
    </root>
  </log4net>
<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
